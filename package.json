{
  "name": "@trimm/tus-client",
  "version": "0.4.0",
  "description": "A Tus.io client... as a binary file",
  "main": "src/app.js",
  "bin": "src/app.js",
  "author": "Alessio Vertemati",
  "license": "MIT",
  "dependencies": {
    "cli-color": "^1.2.0",
    "commander": "~2.9.0",
    "lodash.assignin": "^4.2.0",
    "mime": "^1.4.0",
    "tus-js-client": "^1.4.3"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "pkg": "~4.2.0"
  },
  "scripts": {
    "run": "node ./src/app.js",
    "lint": "eslint src/**/*",
    "lint:fix": "eslint src/**/* --fix",
    "production": "pkg --out-dir=dist . "
  },
  "pkg": {
    "assets": [
      "resources/**/*",
      "package.json"
    ],
    "targets": [
      "node8-macos-x64",
      "node8-linux-x64",
      "node8-win-x64"
    ]
  }
}
